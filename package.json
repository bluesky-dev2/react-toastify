{
  "name": "react-toastify",
  "version": "1.4.3",
  "description": "React notification made easy",
  "keywords": [
    "react",
    "notification",
    "toast",
    "react-component",
    "react-toastify",
    "push",
    "alert"
  ],
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint src/",
    "build": "NODE_ENV=production ./scripts/build.sh",
    "postbuild": "npm run style",
    "prestyle": "sass src/scss/main.scss dist/ReactToastify.css",
    "style": "postcss --use autoprefixer -b 'last 2 versions' < dist/ReactToastify.css | postcss --use cssnano > dist/ReactToastify.min.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fkhadra/react-toastify.git"
  },
  "author": "Fadi Khadra  <fdkhadra@gmail.com> (https://fkhadra.github.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fkhadra/react-toastify/issues"
  },
  "homepage": "https://github.com/fkhadra/react-toastify#readme",
  "dependencies": {
    "prop-types": "^15.5.8",
    "react-transition-group": "^1.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.2",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "cssnano": "^3.8.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.0.0",
    "jsdom": "^10.1.0",
    "karma": "^1.2.0",
    "mocha": "^3.0.2",
    "postcss": "^5.2.5",
    "postcss-cli": "^3.2.0",
    "react-addons-test-utils": "^15.3.0",
    "sinon": "^2.1.0",
    "webpack": "^2.4.1"
  },
  "peerDependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  }
}
